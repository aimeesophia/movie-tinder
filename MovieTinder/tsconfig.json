{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "outDir": "wwwroot/js/",
    "target": "ES6"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "Content/scripts/*.ts"
  ]
}